{
  "version": 2,
  "builds": [
    {
      "src": "server.js", 
      "use": "@vercel/node"  
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js",
      "methods": ["GET,HEAD,PUT,PATCH,POST,DELETE"],
      "headers":{
        "Access-Control-Allows-Origin":"*"
      }
    }
  ]
  }
  